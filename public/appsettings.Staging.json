{
	"$schema": "./appsettings-schema.json",
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information"
		},
		"Umbraco": {
			"CMS": {
				"Content": {
					"MacroErrors": "Throw"
				},
				"RuntimeMinification": {
					"UseInMemoryCache": true,
					"CacheBuster": "Timestamp"
				}
			}
		}
	}
}