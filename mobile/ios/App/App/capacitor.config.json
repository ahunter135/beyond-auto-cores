{
	"appId": "org.catalyticmastermind.beyondautocore",
	"appName": "Catalytic Mastermind",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"Keyboard": {
			"resize": "native",
			"style": "DARK",
			"resizeOnFullScreen": true
		}
	},
	"server": {
		"url": "http://localhost:8100"
	}
}
